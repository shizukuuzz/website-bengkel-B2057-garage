{
  "expo": {
    "name": "B2057 Garage",
    "slug": "bengkel-app",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/b2057.png",
    "scheme": "bengkelapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/b2057.png",
        "backgroundColor": "#ffffff"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "bengkelapp",
              "host": "verify"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ],
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/b2057.png"
    },

    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/b2057.png",
          "imageWidth": 220,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ]
    ],

    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "frfbqylvqcrtatsceiai"
      }
    },

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
